{
	"codeLens.enable": true,
	"java.referencesCodeLens.enabled": true,
	"java.jdt.ls.vmargs": "-javaagent:/usr/local/share/lombok/lombok.jar",
	"html.validate.html": true,
	"bootstrap-classname.filetypes": [
		"html",
		"php"
	],
	"coc.preferences.formatOnSaveFiletypes": [
		"html",
		"css",
		"markdown",
		"python"
	],
	"emmet.includeLanguages": {"php": "html"},
	"suggest.noselect": false,
	"prettier.tabWidth": 2,
	"prettier.useTabs": true,
	"java.enabled": true,
	"explorer.icon.enableNerdfont": true,
	"explorer.previewAction.onHover": false,
	"explorer.keyMappings.global": {
		"<cr>": ["expandable?", "expand", "open"],
		"v": "open:vsplit"
	},
	"languageserver": {
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cpp", "cuda", "objc", "objcpp", "arduino", "ino"],
			"rootPatterns": [".ccls-root", "compile_commands.json"],
			"initializationOptions": {
				"cache": {
					"directory": ".ccls-cache"
				},
				"client": {
					"snippetSupport": true
				}
			}
		}
	},
	"omnisharp.trace.server": "information",
	"python.linting.enabled": true
}
